page {
  background-color: #f5f8fa;
}

.header {
  padding: 0 28rpx;
  height: 140rpx;
  padding-top: 30rpx;
  background: linear-gradient(to right, #ff65a1, #f78d60);
  position: relative;
}

.header .icon {
  width: 36rpx;
}

.header  .hint {
  padding-left: 14rpx;
  font-size: 30rpx;
  color: #fff;
}

.header .order {
  position: absolute;
  bottom: -46rpx;
  left: 28rpx;
  right: 28rpx;
  background: #fff;
  height: 90rpx;
  border-radius: 3px;
  box-shadow: 1px 2px 1px #eff1f3;
  overflow: hidden;
  padding-left: 40rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 90rpx;
  white-space: nowrap;
}

.box {
  padding: 72rpx 28rpx 28rpx 28rpx;
}

.box .example {
  background-color: #fff;
  border-radius: 4px;
  padding: 0 24rpx;
  overflow: hidden;
  box-shadow: 2px 3px 2px #eff1f3;
  margin-bottom: 26rpx;
}

.box .example .head {
  height: 86rpx;
}

.box .example .border-bottm {
  border-bottom: solid 1px #edf0f2;
}

.box .example .head .icon {
  width: 34rpx;
}

.box .example .head .name {
  padding-left: 14rpx;
  font-size: 28rpx;
  color: #4c4c4c;
}

.box .example .list .item {
  padding: 22rpx 0;
  border-bottom: 1px #e6e6e6 dashed;
}

.box .example .list .item .pic {
  width: 120rpx;
  height: 120rpx;
  border-radius: 3px;
  overflow: hidden;
  background-color: #e6e6e6;
}

.box .example .list .item .pic image {
  height: 100%;
}

.box .example .list .item .info {
  /* height: 120rpx; */
  width: calc(100% - 120rpx);
  padding: 16rpx 0;
  padding-left: 20rpx;
}

.box .example .list .item .info .name {
  font-size: 28rpx;
  color: #4c4c4c;
  width: 70%;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.box .example .list .item .info .price {
  color: #ff336d;
  font-size: 28rpx;
}

.box .example .list .item .info .run {
  font-size: 26rpx;
  color: #b2b2b2;
  white-space: nowrap;
}

.box .example .checkmore {
  font-size: 26rpx;
  color: #ff336d;
  border-bottom: 1px #e6e6e6 dashed;
  text-align: center;
  line-height: 86rpx;
}

.box .example .lins {
  padding: 18rpx 0;
}

.box .example .lins .lin {
  padding: 2rpx 0;
}

.box .example .lins .lin text {
  line-height: 1.5;
  font-size: 26rpx;
}

.box .example .lins .lin .tit {
  color: #808080;
}

.box .example .lins .lin .text {
  color: #666;
}

.box .example .lins .lin.pay .tit {
  color: #4c4c4c;
}

.box .example .lins .lin.pay .text {
  color: #ff336d;
}

.box .example .down-item {
  border-bottom: 1px #e6e6e6 dashed;
}

.box .example .down-item:last-child {
  border-bottom: none;
}

.box .example .msg  .item {
  padding: 4rpx 0;
}
.box .example .express{
  background: linear-gradient(to right,#ff65a0,#f78d61);
  color: #fff;
  font-size: 26rpx;
  border-radius: 3px;
  line-height: 1.5;
  padding: 6rpx 16rpx;
}
.box .example .msg {
  padding-bottom: 26rpx;
  padding-left: 48rpx;
  font-size: 26rpx;
}

.box .example .msg .tit {
  color: #999;
}

.box .example .msg .text {
  color: #666;
}